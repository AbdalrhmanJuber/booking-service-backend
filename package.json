{
  "name": "Booking service",
  "version": "1.0.0",
  "description": "Booking service api",
  "license": "ISC",
  "author": "Abdelrahman Jaber",
  "main": "server.ts",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "ts-node node_modules/jasmine/bin/jasmine.js",
    "pretest": "db-migrate reset --config ./database.js --env test && db-migrate up --config ./database.js --env test",
    "build": "tsc",
    "migrate:up": "db-migrate up --config ./database.js",
    "migrate:down": "db-migrate down --config ./database.js",
    "migrate:create": "db-migrate create --config ./database.js",
    "migrate:test": "db-migrate up --config ./database.js --env test",
    "debug": "ndb src/server.js"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jasmine": "^5.1.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/nodemailer": "^7.0.3",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "jasmine": "^5.12.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3"
  }
}
